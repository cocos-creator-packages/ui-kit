{
  "name": "ui-kit",
  "version": "0.1.2",
  "description": "Editor Framework UI Kit",
  "author": "Firebox Technology",
  "build": true,
  "hosts": {
    "editor-framework": "^0.3.14"
  },
  "main": "main.js",
  "menus": {
    "Developer/UI Preview/ui-kit": {
      "message": "ui-kit:open"
    }
  },
  "widgets": {
    "editor-button": "widget/button/button.html",
    "editor-buttons": "widget/buttons/buttons.html",
    "editor-checkbox": "widget/checkbox/checkbox.html",
    "editor-input": "widget/input/input.html",
    "editor-label": "widget/label/label.html",
    "editor-loader": "widget/loader/loader.html",
    "editor-select": "widget/select/select.html",
    "editor-slider": "widget/slider/slider.html",
    "editor-unit-input": "widget/unit-input/unit-input.html"
  },
  "panels": {
    "panel": {
      "frame": "panel/panel.html",
      "type": "dockable",
      "title": "UI Kit Preview",
      "width": 800,
      "height": 600,
      "messages": []
    }
  },
  "tests": [
    "test/button.html",
    "test/buttons.html",
    "test/checkbox.html",
    "test/input.html",
    "test/label.html",
    "test/loader.html",
    "test/select.html",
    "test/slider.html",
    "test/unit-input.html"
  ]
}